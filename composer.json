{
  "name": "captcha-eu/typo3-powermail",
  "description": "Captcha.eu TYPO3 extension for powermail",
  "keywords": [
    "typo3",
    "powermail",
    "captcha",
    "captcha-eu"
  ],
  "homepage": "https://github.com/captcha-eu/typo3-powermail",
  "authors": [
    {
      "name": "Helmut Januschka",
      "email": "h.januschka@captcha.eu",
      "role": "CEO",
      "homepage": "https://www.captcha.eu"
    }
  ],
  "type": "typo3-cms-extension",
  "license": "GPL-2.0-or-later",
  "require": {
    "in2code/powermail": "^12.0 | ^11.0 | dev-develop",
    "ext-json": "*"
  },
  "replace": {
    "typo3-ter/captchaeu": "self.version"
  },
  "autoload": {
    "psr-4": {
      "CaptchaEU\\": "Classes"
    }
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "captchaeu"
    }
  }
}
